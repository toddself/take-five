{
  "compilerOptions": {
    "allowJs": true,
    "esModuleInterop": true,
    "lib": ["esnext"],
    "module": "esnext",
    "moduleResolution": "node",
    "noEmit": true,
    "pretty": true,
    "resolveJsonModule": true,
    "target": "esnext",
    "noImplicitThis": false,
    "paths": {
      "https://*": ["../../Library/Caches/deno/deps/https/*"]
    },
    "plugins": [{
      "name": "typescript-deno-plugin",
      "enable": true,
      "importmap": "import_map.json"
    }]
  },
  "include": [
    "./**/*.ts"
  ]
}
