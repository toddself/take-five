{
  "compilerOptions": {
    "allowJs": true,
    "esModuleInterop": true,
    "lib": ["esnext"],
    "module": "esnext",
    "moduleResolution": "node",
    "noEmit": true,
    "plugins": [ { "name": "deno_ls_plugin" } ],
    "pretty": true,
    "resolveJsonModule": true,
    "target": "esnext",
    "paths": {
      "https://*": ["../../Library/Caches/deno/deps/https/*"]
    }
  },
  "include": [
    "./**/*.ts"
  ]
}
